sudo apt-get update
sudo apt-get install nginx -y
sudo systemctl start nginx
sudo systemctl enable nginx
sudo systemctl status nginx
cd /var/www/html
ls
sudo chmod -R 777 ./
sudo nano /var/www/html/index.html

#HTML Code
<html>
    <head>
        <title>Welcome to Nginx Web Server</title>
    </head>
    <body>
        <h1>Hello World</h1>
    </body>
</html>

